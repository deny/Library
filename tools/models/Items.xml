<?xml version="1.0" encoding="UTF-8"?>

<Items use="Friends">
	<Item>
		<field name="name"			type="string(128)"			access="get,set" />
		<field name="category" 		type="Category" 			access="get,set" />
		<field name="description"	type="string"				access="get,set" />
		<field name="status"		type="enum(free,borrowed)"	access="get,set" />
	</Item>
	<Category>
		<field name="name"			type="string(128)"			access="get,set" />
	</Category>
	<Borrow>
		<field name="item"			type="Item"					access="get,set" />
		<field name="friend"		type="Friends:Friend"		access="get,set" />
		<field name="start"			type="int"					access="get,set" />
		<field name="end"			type="int"					access="get,set" />
	</Borrow>
</Items>